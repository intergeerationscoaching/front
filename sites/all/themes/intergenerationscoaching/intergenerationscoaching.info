name = Intergenerations Coaching
description = Uses the jsDelivr CDN for all CSS and JavaScript. No source files or compiling is necessary and is recommended for simple sites or beginners.
core = 7.x
base theme = bootstrap


;;;;;;;;;;;;;;;;;;;;;
;; Regions
;;;;;;;;;;;;;;;;;;;;;

regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[highlighted]    = 'Highlighted'
regions[header_right]   = 'Header Right'
regions[content]        = 'Content'
regions[sidebar_first]  = 'Primary'
regions[sidebar_second] = 'Secondary'
regions[footer]         = 'Footer'
regions[page_top]       = 'Page top'
regions[page_bottom]    = 'Page bottom'

regions[notification_bar] = 'Notification Panel'
regions[modal_form]       = 'Modal'
regions[footer_left]      = 'Footer Left'
regions[footer_right]     = 'Footer Right'




;;;;;;;;;;;;;;;;;;;;;
;; Stylesheets
;;;;;;;;;;;;;;;;;;;;;


stylesheets[all][] = css/style.css
stylesheets[all][] = css/responsive.css
stylesheets[all][] = css/jquery.sidr.dark.min.css


scripts[] = js/main.js
scripts[] = js/jquery.sidr.min.js

;;;;;;;;;;;;;;;;;;;;;
;; Excludes
;;;;;;;;;;;;;;;;;;;;;

; JavaScript
; --------------
exclude[js][] = 'sites/all/themes/bootstrap/js/modules/user/user.js'


exclude[css][] = 'sites/all/themes/bootstrap/css/3.3.7/overrides.min.css'
